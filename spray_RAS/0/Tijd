FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Tijd;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    in_air
    {
        type            fixedValue;
        value           uniform 0;
    }

    out_air
    {
        type            zeroGradient;
    }

    walls
    {
        type            zeroGradient;
    }

    out_part
    {
        type            zeroGradient;
    }

    in_part
    {
        type            zeroGradient;
    }
}

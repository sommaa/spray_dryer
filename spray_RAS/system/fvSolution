FoamFile {
    version 2.0;
    format ascii;
    class dictionary;
    object fvSolution;
}


solvers
{
    p
    {
        solver          GAMG;
        tolerance       1e-5;
        relTol          0.1;
        smoother        GaussSeidel;
    }

    pFinal
    {
        solver          GAMG;
        tolerance       1e-5;
        relTol          0.1;
        smoother        GaussSeidel;
    }

    "(omega|U|k|nuTilda|tracer)"
    {
        solver          smoothSolver;
        smoother        symGaussSeidel;
        tolerance       1e-05;
        relTol          0.1;
    }

    "(omega|U|k|nuTilda|tracer)Final"
    {
        $U;
        tolerance       1e-05;
        relTol          0.1;
    }
    
}

PIMPLE
{
    nOuterCorrectors 1;
    nCorrectors     4;
    nNonOrthogonalCorrectors 0;
    pRefCell        0;
    pRefValue       0;
    correctPhi off;
}

